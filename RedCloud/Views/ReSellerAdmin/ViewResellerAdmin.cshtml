@model IEnumerable<RedCloud.Models.ResellerAdminVM>


@{
    ViewData["Title"] = "Index";
}

<div class="card">
    <div class="card-header">
        <h3 class="card-title">Admin Reseller</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
            <div class="row">
                <div class="col-sm-12">
                    <table id="dataTable" class="table table-bordered table-striped dataTable dtr-inline" aria-describedby="dataTable_info">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>CompanyName</th>
                                <th>EIN</th>
                                <th>Email</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>

                                    @foreach (var admins in Model)
                                    {
                                        <tr>
                                            <td>@admins.Id</td>
                                            <td>@admins.ReSellerName</td>
                                            <td>@admins.EIN</td>
                                            <td>@admins.CompanySupportEmail</td>
                                            <td>@((bool)admins.IsActive ? "Active" : "Not Active")</td>
                                            <td>
                                        <a asp-action="GetResellerAdminById" asp-controller="ReSellerAdmin" asp-route-id="@admins.Id">
                                            <i class="fa-solid fa-eye" style="color: #050505;"></i>

                                </a>
                                        <a asp-action="" asp-controller=""><i class="fa-solid fa-plus" style="color: #000000;"></i></a>
                                        <a asp-action="" asp-controller=""><i class="fa-regular fa-pen-to-square" style="color: #131111;"></i></a>
                                        <a class="delete-btn" data-id="@admins.Id" data-name="@admins.ReSellerName" href="#"><i class="fa-solid fa-trash" style="color: #cb1010;"></i></a>
                                            </td>

                                        </tr>
                                    }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!-- /.card-body -->
</div>

<div class="modal fade" id="deleteConfirmationModal" tabindex="-1" role="dialog" aria-labelledby="deleteConfirmationModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="deleteConfirmationModalLabel">Confirm Delete</h5>
                <button type="button" class="close" data-dismiss="modal" id="cancelBtn" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>Are you sure you want to delete <span id="deleteItemName"></span>?</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" id="confirmDeleteBtn">Delete</button>
            </div>
        </div>
    </div>
</div>

